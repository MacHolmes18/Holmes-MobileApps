            android:parentActivityName="com.example.holmesintentexercise.MainActivity"
            >
            <meta-data
                android:name = "android.support.PARENT_ACTIVITY"
                android:value = "com.example.holmesintentexercise.MainActivity"
             />
        </activity>
        
        
    </application>

</manifest>
